{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "outDir": "dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@types"],
    "types": ["node", "express"],
    "moduleResolution": "node"
  },
  "include": ["api/**/*.ts", "prisma/**/*.ts", "./index.ts"],
  "exclude": ["node_modules"]
}
