{
  "name": "srcbook-app",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "tsx watch --loader ts-node/esm server/index.ts",
    "dev:all": "concurrently -n \"frontend,backend\" \"pnpm run dev\" \"pnpm run server\"",
    "init-db": "prisma generate && prisma migrate reset --force",
    "db:reset": "prisma migrate reset --force && prisma generate && prisma db seed",
    "vercel-build": "prisma generate && prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "@types/react-input-mask": "^3.0.6",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "lucide-react": "^0.453.0",
    "prisma": "^6.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^7.1.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "@vercel/node": "^5.0.2",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "globals": "^15.9.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.5.3",
    "vite": "^5.4.6"
  },
  "packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4"
}
